    "@docs-hound/shared-db": "workspace:*",
    "@docs-hound/tool-docs-search": "workspace:*",
    "react-markdown": "^10.1.0",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1"
  }
}
